@model Student
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detail</title>
</head>
<body>
    <h1>学生信息</h1>
    <h2>ID：@Model.Id</h2>
    <h2>Name:@Model.FirstName- @Model.LastName</h2>
    <h2>BirthDate:@Model.BirthDate.ToString("yyyy-MM-dd")</h2>
    <h2>Gender:@Model.gender</h2>
    <a asp-action="Index">返回到Home</a>
</body>
</html>
